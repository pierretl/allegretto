Options -Indexes
#ErrorDocument 403 https://www.pierretl.fr/allegretto/
#ErrorDocument 404 https://www.pierretl.fr/allegretto/index.php?p=404

<Files ~ ".env">
  Order Allow,Deny
  Deny from all
</Files>

RewriteEngine on
RewriteCond %{SERVER_PORT} 80
RewriteRule .* https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]